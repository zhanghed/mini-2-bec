/*排序界面*/

.set-b {
   position: fixed;
   bottom: 0rpx;
   display: flex;
   flex-direction: row;
   flex-wrap: wrap;
   justify-content: space-between;
   align-items: center;
   padding: 0rpx 10rpx 0rpx 0rpx;
   background-color: rgba(221, 229, 245, 0.342);
   border: 1rpx solid #88878777;
  box-shadow: 6rpx 6rpx 6rpx #cfcfcf77;
}

.bb {
   font-size: 28rpx;
   
