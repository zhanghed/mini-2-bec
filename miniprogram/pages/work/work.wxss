.abc {
   height: 100vh;
   width: 100vw;
   font-family: "Times New Roman", Microsoft YaHei;
   font-weight: 500;
}

.ok {
   font-size: 20rpx;
   width: 100%;
   text-align: center;
}

.mask {
   width: 100%;
   height: 100%;
   position: fixed;
   z-index: 9990;
   top: 0;
   left: 0;
   opacity: 0.5;
   background-color: rgb(0, 0, 0);
}


/* 按钮 */

.main {
   width: 100%;
   height: 5%;
   display: flex;
   flex-direction: row;
   flex-wrap: wrap;
   justify-content: space-around;
   align-items: flex-end;
   margin-top: 20rpx;
   padding-bottom: 30rpx;
   box-shadow: 0rpx -2rpx 1rpx #cfcfcf inset;
}

.main button {
   font-size: 32rpx;
   line-height: 70rpx;
   width: 190rpx;
   height: 70rpx;
  
   background-color: rgba(221, 229, 245, 0.342);
   border: 1rpx solid #88878777;

}



