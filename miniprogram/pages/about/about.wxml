<view class="main">
	<scroll-view scroll-y>
		<view class="abc">

			<view class="logo">
				<image class="logo-l" src='/images/log.png'></image>

			</view>

			<view class="main-i" bindtap="to_work" hover-class='cccc' hover-start-time='10' hover-stay-time='40' hover-stop-propagation='{{true}}'>
				<image src='/images/1.svg' style='width: 45rpx;height: 45rpx;'></image>
				<view class="txt">
					规则设置
				</view>
				<image class="img" src='/images/go.svg'></image>
			</view>

			<view class="main-i" bindtap="to_note" hover-class='cccc' hover-start-time='10' hover-stay-time='40' hover-stop-propagation='{{true}}'>
				<image src='/images/1.svg' style='width: 45rpx;height: 45rpx;'></image>
				<view class="txt">
					记事管理
				</view>
				<image class="img" src='/images/go.svg'></image>
			</view>

			<view class="main-i" bindtap="to_illustrate" hover-class='cccc' hover-start-time='10' hover-stay-time='40' hover-stop-propagation='{{true}}'>
				<image src='/images/1.svg' style='width: 45rpx;height: 45rpx;'></image>
				<view class="txt">
					程序说明
				</view>
				<image class="img" src='/images/go.svg'></image>
			</view>

			<view class="main-i" bindtap="to_me" hover-class='cccc' hover-start-time='10' hover-stay-time='40' hover-stop-propagation='{{true}}'>
				<image src='/images/1.svg' style='width: 45rpx;height: 45rpx;'></image>
				<view class="txt">
					联系作者
				</view>
				<image class="img" src='/images/go.svg'></image>
			</view>
			
		</view>
	</scroll-view>
</view>